{
    "name": "ng4-input-slider",
    "version": "1.0.0",
    "private": false,
    "description": "Slider component for Angular 4+",
    "keywords": [
        "slider",
        "component",
        "angular4",
        "ng4",
        "angular5",
        "ng5"
    ],
    "author": "Aqdas Malik",
    "license": "MIT",
    "repository": {
        "type": "git",
        "url": "https://github.com/aqdas95/ng4-inputSlider.git"
    },
    "bugs": {
        "url": "https://github.com/aqdas95/ng4-inputSlider/issues"
    },
    "homepage": "https://github.com/aqdas95/ng4-inputSlider#readme",
    "scripts": {
        "prepare": "npm run prepare:lib && npm run prepare:demo-app",
        "prepare:lib": "node scripts/generate-lib-files.js",
        "prepare:demo-app": "node scripts/generate-demo-app-snippets.js && node scripts/generate-demo-app-docs.js",
        "start": "npm run prepare && ng serve",
        "build": "npm run build:lib && npm run build:demo-app",
        "build:lib": "npm run prepare && rimraf dist/ng4-input-slider dist/ng4-input-slider.tgz && ng-packagr -p src/ng4-input-slider/package.json && scss-bundle -e src/ng4-input-slider/lib/main.scss -d dist/ng4-input-slider/scss/ng4-input-slider.scss",
        "build:demo-app": "npm run prepare && rimraf dist/demo-app && ng build --prod --base-href /ng4-input-slider/",
        "publish-gh": "scripts/publish-gh-pages.sh",
        "test": "npm run prepare && ng test",
        "test:once": "npm run prepare && ng test --watch=false",
        "lint": "npm run prepare && ng lint",
        "e2e": "npm run prepare && ng e2e",
        "ci": "npm run test:once && npm run e2e && npm run lint && npm run build:lib && npm run build:demo-app"
    },
    "dependencies": {
        "@angular/animations": ">=4.2.4",
        "@angular/common": ">=4.2.4",
        "@angular/compiler": ">=4.2.4",
        "@angular/core": ">=4.2.4",
        "@angular/forms": ">=4.2.4",
        "@angular/http": ">=4.2.4",
        "@angular/platform-browser": ">=4.2.4",
        "@angular/platform-browser-dynamic": ">=4.2.4",
        "@angular/router": ">=4.2.4",
        "@ng-bootstrap/ng-bootstrap": "^1.0.0",
        "core-js": "^2.4.1",
        "detect-passive-events": "^1.0.4",
        "rxjs": "^6.4.0",
        "typedoc": "^0.14.2",
        "typescript": "^3.3.4000",
        "zone.js": "^0.8.19"
    },
    "devDependencies": {
        "@angular/cli": "~1.7.3",
        "@angular/compiler": "^4.2.4",
        "@angular/compiler-cli": "^4.2.4",
        "@angular/language-service": "^4.2.4",
        "@types/jasmine": "~2.8.3",
        "@types/jasminewd2": "~2.0.2",
        "@types/node": "~6.0.60",
        "codelyzer": "^4.0.1",
        "escape-html": "^1.0.3",
        "fs": "0.0.1-security",
        "jasmine-core": "~2.8.0",
        "jasmine-spec-reporter": "~4.2.1",
        "karma": "~2.0.0",
        "karma-chrome-launcher": "~2.2.0",
        "karma-coverage-istanbul-reporter": "^1.2.1",
        "karma-jasmine": "~1.1.0",
        "karma-jasmine-html-reporter": "^0.2.2",
        "mkdirp": "^0.5.1",
        "ng-packagr": "^2.4.2",
        "path": "^0.12.7",
        "prismjs": "^1.14.0",
        "protractor": "~5.1.2",
        "protractor-console-plugin": "^0.1.1",
        "rimraf": "^2.6.2",
        "scss-bundle": "^2.4.0",
        "ts-node": "~4.1.0",
        "tsickle": "~0.25.5",
        "tslint": "~5.9.1"
    }
}